@{
    Layout = null;
}
<div class="close-modal" data-dismiss="modal">
    <i class="fa fa-3x fa-close"></i>
</div>
<div class="container">

    <h3 class="customer-h3 text-center curd-title">新 增</h3>
    <hr />
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-2 control-label">
                <label class="customer-content-sm">类别代码：</label>
            </div>
            <div class="col-md-10">
                <input class="input-large" id="tbTypeCode" placeholder="类别代码" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-2 control-label">
                <label class="customer-content-sm">类别名称：</label>
            </div>
            <div class="col-md-10">
                <input class="input-large" id="tbTypeName" placeholder="类别名称" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-2 control-label">
                <label class="customer-content-sm">顺 序：</label>
            </div>
            <div class="col-md-10">
                <input class="input-large" id="tbSequence" placeholder="顺 序" />
            </div>
        </div>
    </div>

</div>
<div class="modal-footer">
    <button type="button" class="btn btn-lg btn-info"
            data-dismiss="modal">
        退 出
    </button>
    <button type="button" class="btn btn-lg btn-danger" onclick="SaveAdd()">
        保 存
    </button>
</div>