<?xml version="1.0" encoding="utf-8"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    
    <section name="RewriterConfig" type="URLRewriter.Config.RewriterConfigSerializerSectionHandler, URLRewriter" />
  </configSections>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Server = localhost\SQLEXPRESS;Database=HardaGroupDB;User ID = HardaAdmin;Password = Admin_1234;Trusted_Connection = False;" providerName="System.Data.SqlClient" />

    
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <add key="UrlRewriteExtension" value=".html" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <!--此301跳转-->
    <rewrite>
      <rules>
        <rule name="Redirect to www">
          <match url=".*" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="^ahllen.net"/>
          </conditions>
          <action type="Redirect" url="http://www.ahllen.net/{R:0}" redirectType="Permanent"/>
        </rule>
      </rules>
    </rewrite>
    
    <modules>
      
      <!--URL重写-->
      <add type="URLRewriter.ModuleRewriter, URLRewriter" name="ModuleRewriter" />
      <!--多语言全球化-->
      <add name="CultureAwareHttpModule" type="HardaGroup.Web.Globalization.CultureAwareHttpModule,HardaGroup.Web"/>

      <remove name="FormsAuthentication" />
    </modules>
    <handlers>
      <!--如果改变此处的path值，则要修改appSettings中相应的配置-->
      <add name="Html-Integrate" path="*.html" verb="GET,HEAD,POST,DEBUG" type="System.Web.UI.PageHandlerFactory" resourceType="Unspecified" requireAccess="Script" preCondition="integratedMode" />

    </handlers>
    
  </system.webServer>
  
  <!--URL重写配置-->
  <RewriterConfig>
    <Rules>
      <!--关于华尔达 -->
      <RewriterRule>
        <LookFor>~/about.html</LookFor>
        <SendTo>~/Home/About</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/about.html</LookFor>
        <SendTo>~$1/Home/About</SendTo>
      </RewriterRule>
      <!--智能制造-->
      <RewriterRule>
        <LookFor>~/advanced-manufacturing.html</LookFor>
        <SendTo>~/AdvancedManufacturing/Index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/advanced-manufacturing.html</LookFor>
        <SendTo>~$1/AdvancedManufacturing/Index</SendTo>
      </RewriterRule>
      <!--解决方案-->
      <RewriterRule>
        <LookFor>~/solutions/parts-and-modules.html</LookFor>
        <SendTo>~/Solutions/PartsAndModules</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/solutions/parts-and-modules.html</LookFor>
        <SendTo>~$1/Solutions/PartsAndModules</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/solutions/research-and-development.html</LookFor>
        <SendTo>~/Solutions/ResearchAndDevelopment</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/solutions/research-and-development.html</LookFor>
        <SendTo>~$1/Solutions/ResearchAndDevelopment</SendTo>
      </RewriterRule>
      
      <!--创新创业-->
      <RewriterRule>
        <LookFor>~/venture-support.html</LookFor>
        <SendTo>~/VentureSupport/Index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/venture-support.html</LookFor>
        <SendTo>~$1/VentureSupport/Index</SendTo>
      </RewriterRule>
      <!--媒体资讯-->
      <RewriterRule>
        <LookFor>~/press.html</LookFor>
        <SendTo>~/Press/index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/press.html</LookFor>
        <SendTo>~$1/Press/index</SendTo>
      </RewriterRule>
      <!--加入我们-->
      <RewriterRule>
        <LookFor>~/join-us.html</LookFor>
        <SendTo>~/Home/joinus</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/join-us.html</LookFor>
        <SendTo>~$1/home/joinus</SendTo>
      </RewriterRule>

      <!--联系我们 -->
      <RewriterRule>
        <LookFor>~/contact-us.html</LookFor>
        <SendTo>~/Home/ContactUs</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/contact-us.html</LookFor>
        <SendTo>~$1/Home/ContactUs</SendTo>
      </RewriterRule>
      

      <!-- 后台管理URL -->
      <RewriterRule>
        <LookFor>~/manage.html</LookFor>
        <SendTo>~/manage/index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/manage.html</LookFor>
        <SendTo>~$1/manage/index</SendTo>
      </RewriterRule>
      
      <RewriterRule>
        <LookFor>~/manage/([A-Za-z]+).html</LookFor>
        <SendTo>~/manage/$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/manage/([A-Za-z]+).html</LookFor>
        <SendTo>~$1/manage/$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/manage/background-([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~/manage/backgroundimage?type=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/manage/background-([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~$1/manage/backgroundimage?type=$2</SendTo>
      </RewriterRule>
    </Rules>
  </RewriterConfig>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security.OAuth" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security.Cookies" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Optimization" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-1.1.0.0" newVersion="1.1.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>